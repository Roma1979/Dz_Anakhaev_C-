using System;
 
namespace HelloApp
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите число от одного до семи: ");
            int num1 = Convert.ToInt32(Console.ReadLine());
            
            if(num1 < 1 || num1 >7) 
            {
                Console.WriteLine("Неправильно ввели число");
                
            }
            else if (num1 > 5)
            {
                Console.WriteLine("Да выходной день");
            }
            else if (num1 <= 5) 
            {
                Console.WriteLine("Нет не выходной");
            }

            Console.ReadKey();
        }
    }
}